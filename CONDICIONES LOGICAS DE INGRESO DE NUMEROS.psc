Proceso sin_titulo
	
	
	Definir  numero1 como entero;
	
	numero1 <- [10];
	
	escribir "Ingrese un numero";
	leer numero1;
	
	si numero1 = 7 Entonces
		
		escribir Verdadero;
		
	SiNo
		
		si numero1 = 0 Entonces
			
			escribir falso;
			
		FinSi
		
		si numero1 <> 7 Entonces
			
			escribir "El numero ingresado es distinto de 7";
			Esperar Tecla;
			
		FinSi
		
	FinSi
	
	escribir "";
	
	escribir "Fin del programa";
	Esperar Tecla;
	
FinProceso
